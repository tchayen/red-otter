{
  "name": "@red-otter/docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "tsc": "tsc",
    "build": "yarn font-atlas && yarn generate && vite build",
    "serve": "vite preview",
    "watch": "nodemon -w src -e ts,tsx --exec 'yarn generate'",
    "generate": "yarn run ts-node --esm --swc --experimentalSpecifierResolution=node src/build.ts",
    "font-atlas": "yarn run ts-node --esm --swc --experimentalSpecifierResolution=node font-atlas/generate.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-typescript": "^7.21.0",
    "@sparticuz/chromium": "^112.0.2",
    "@swc/core": "^1.3.41",
    "@types/babel__core": "^7.20.0",
    "@types/node": "^18.15.5",
    "@types/prettier": "^2.7.2",
    "chalk": "^4.1.2",
    "highlight.js": "^11.7.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.6",
    "puppeteer-core": "^19.7.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1"
  }
}
