{
  "name": "@red-otter/monorepo",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "scripts": {
    "test": "vitest --no-threads",
    "docs:dev": "cd docs; yarn run vite dev",
    "docs:build": "cd docs; yarn run vite build",
    "docs:generate": "yarn run ts-node  -O '{\"module\":\"nodenext\"}' docs/build.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
  }
}
